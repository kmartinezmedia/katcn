{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".turbo-env",
    "package.json",
    "biome.json",
    "tsconfig.json"
  ],
  "tasks": {
    "build": {
      "outputs": [
        "dist/**",
        "build/**",
        ".next/**",
        "!.next/cache/**",
        ".katcn/**",
        "bin"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "build:icons": {
      "inputs": [
        "src/icons/svg/**"
      ],
      "outputs": [
        "src/icons/fonts/**",
        "src/icons/types.ts",
        "src/fixtures/icons.ts"
      ],
      "dependsOn": []
    },
    "build:css": {
      "outputs": [
        ".katcn/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    },
    "server:start": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    },
    "server:restart": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "fix": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": [
        "^build",
        "build:css"
      ]
    },
    "test": {
      "outputs": [],
      "dependsOn": []
    },
    "test:update": {
      "outputs": [],
      "dependsOn": []
    },
    "test:coverage": {}
  }
}
