{
  "name": "katcn-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo run build --log-order=grouped",
    "clean": "turbo run clean --log-order=grouped && rm -rf node_modules && rm -rf .turbo",
    "dev": "turbo run dev",
    "fix": "turbo run fix --log-order=grouped",
    "lint": "turbo run lint --log-order=grouped",
    "test": "turbo run test --log-order=grouped",
    "test:update": "turbo run test:update --log-order=grouped",
    "test:watch": "turbo run test -- --watch --log-order=grouped",
    "typecheck": "turbo run typecheck --log-order=grouped",
    "update-deps": "bun update --interactive --recursive"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/bun": "^1.3.0",
    "@types/node": "^24.0.0",
    "cli": "workspace:*",
    "turbo": "canary",
    "tsconfig": "workspace:*",
    "typescript": "catalog:"
  },
  "catalog": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/base64-url": "^2.2.2",
    "@types/lodash": "^4.17.20",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "autoprefixer": "^10.4.21",
    "bluebun": "^0.0.34",
    "clsx": "^2.1.0",
    "concurrently": "^9.2.0",
    "@expo/eas-json": "^16.23.0",
    "lodash": "^4.17.21",
    "motion": "^12.23.0",
    "next": "^15.5.6",
    "next-themes": "^0.4.6",
    "prettier": "^3.6.2",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "server-only": "^0.0.1",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "tailwind-merge": "^3.0.0",
    "ts-morph": "^27.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "bun@1.3.00",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "trustedDependencies": [
    "@biomejs/biome",
    "tailwindcss"
  ]
}
