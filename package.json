{
  "name": "katcn-monorepo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules && rm -rf .turbo",
    "dev": "turbo run dev",
    "fix": "turbo run fix",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "test": "turbo run test",
    "test:watch": "turbo run test -- --watch",
    "typecheck": "turbo run typecheck",
    "update-deps": "syncpack update --config ./cli/src/commands/tools/.syncpackrc"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "turbo": "^1.13.2",
    "typescript": "^5.4.2"
  },
  "packageManager": "bun@1.1.31",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "trustedDependencies": [
    "@biomejs/biome"
  ]
}
