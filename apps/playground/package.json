{
  "name": "playground",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "module": "src/index.ts",
  "scripts": {
    "clean": "cli tools clean",
    "css": "katcn css --include docgen --outFile ./.katcn/styles.css",
    "init": "bun run ./scripts/init.ts",
    "dev": "NODE_ENV=production bun run init && NODE_ENV=production bun run --hot src/index.tsx",
