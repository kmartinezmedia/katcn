    "lint": "cli tools lint",
    "test": "bun test",
    "typecheck": "cli tools typecheck"
  },
  "dependencies": {
    "hono": "^4.0.10",
    "katcn": "workspace:*"
  },
  "devDependencies": {
    "tsconfig": "workspace:*"
  }
}
