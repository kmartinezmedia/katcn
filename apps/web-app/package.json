{
  "name": "web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev",
    "build": "bun run docgen && bun --bun next build",
    "docgen": "cli docs build --input katcn",
    "start": "next start",
    "lint": "cli tools lint",
    "typecheck": "cli tools typecheck",
    "test": "bun test",
    "fix": "cli tools fix",
    "clean": "cli tools clean"
  },
  "dependencies": {
    "katcn": "workspace:*",
    "react": "^18",
    "react-dom": "^18",
    "next": "14.1.1"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.18",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsconfig": "workspace:*"
  },
  "imports": {
    "#docgen/*": {
      "types": "./.docgen/*.ts",
      "default": "./.docgen/*.ts"
    }
  }
}
