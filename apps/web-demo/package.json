{
  "name": "web-demo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "scripts": {
    "build": "bun run css && next build",
    "clean": "cli tools clean",
    "css": "katcn css --outFile ./.katcn/styles.css",
    "dev": "concurrently bun:dev:*",
    "dev:css": "bun run css --watch",
    "dev:next": "bun --bun next dev",
    "docgen": "cli docs build --input katcn",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "start": "next start",
    "test": "bun test",
    "typecheck": "cli tools typecheck"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@types/apca-w3": "^0.1.3",
    "@types/culori": "^2.1.0",
    "@types/wcag-contrast": "^3.0.3",
    "apca-w3": "^0.1.9",
    "base64-url": "^2.3.3",
    "culori": "^4.0.1",
    "framer-motion": "^11.0.23",
    "jotai": "^2.7.2",
    "katcn": "workspace:*",
    "monaco-editor": "^0.47.0",
    "next": "14.2.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server": "workspace:*",
    "wcag-contrast": "^3.0.0"
  },
  "devDependencies": {
    "@types/base64-url": "^2.2.2",
    "@types/react": "~18.3.0",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "tsconfig": "workspace:*"
  },
  "imports": {
    "#docgen/*": {
      "types": ".docgen/*.ts",
      "default": ".docgen/*.ts"
    },
    "#katcn/*.css": ".katcn/*.css"
  }
}
