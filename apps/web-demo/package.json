{
  "name": "web-demo",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json"
  },
  "scripts": {
    "build": "next build",
    "clean": "cli tools clean",
    "dev": "concurrently bun:dev:*",
    "dev:css": "katcn css --watch",
    "dev:next": "next dev",
    "docgen": "cli docs build --input katcn",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "start": "next start",
    "test": "bun test",
    "typecheck": "cli tools typecheck"
  },
  "dependencies": {
    "docgen": "workspace:*",
    "katcn": "workspace:*",
    "next": "14.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "next",
    "@tailwindcss/postcss": "next",
    "@types/react": "~18.2.61",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "tailwindcss": "next",
    "tsconfig": "workspace:*"
  },
  "imports": {
    "#docgen/*": {
      "types": ".docgen/*.ts",
      "default": ".docgen/*.ts"
    },
    "#css": {
      "types": ".katcn/css.ts",
      "default": ".katcn/css.tsx"
    }
  }
}
