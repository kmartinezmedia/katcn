{
  "name": "katcn",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": ["package.json", "tsconfig.json", "src", "dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tailwind": {
      "types": "./dist/tailwind.d.ts",
      "default": "./dist/tailwind.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "default": "./dist/jsx-dev-runtime.js"
    },
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsup && bun run ./scripts/postbuild.ts",
    "build:icons": "cli icons build --formats woff2,ttf --input ./src/icons/svg --outputFonts ./src/icons/fonts --outputTypes ./src/icons/types.ts",
    "clean": "cli tools clean",
    "dev": "concurrently bun:dev:*",
    "dev:icons": "bun run build:icons --watch",
    "dev:src": "tsup --watch",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "test": "bun test",
    "typecheck": "cli tools typecheck"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-typescript": "^7.23.3",
    "clsx": "^2.1.0",
    "tailwindcss": "^3.4.1",
    "tailwind-merge": "^2.2.1",
    "ts-morph": "^21.0.1",
    "cli": "workspace:*",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2",
    "webfont": "^11.2.26",
    "tsconfig": "workspace:*"
  }
}
