{
  "name": "katcn",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": ["package.json", "tsconfig.json", "src", "dist", "css"],
  "style": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./getStyles": {
      "types": "./dist/getStyles.d.ts",
      "default": "./dist/getStyles.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "default": "./dist/jsx-dev-runtime.js"
    },
    "./base.css": "./css/base.css",
    "./base": "./css/base.css",
    "./preflight.css": "./css/preflight.css",
    "./preflight": "./css/preflight.css",
    "./theme.css": "./css/theme.css",
    "./theme": "./css/theme.css",
    "./utilities.css": "./css/utilities.css",
    "./utilities": "./css/utilities.css",
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "imports": {
    "#extractStyleProps": {
      "types": "./src/extractStyleProps.ts",
      "default": "./dist/extractStyleProps.js"
    },
    "#getStyles": {
      "types": "./src/getStyles.ts",
      "default": "./dist/getStyles.js"
    },
    "#css": "./src/css/index.ts",
    "#macros": "./src/macros/index.ts",
    "#macros/*": "./src/macros/*.ts",
    "#tokens/*": "./src/tokens/*.ts"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsup && bun run ./scripts/postbuild.ts && bun run build:css",
    "build:css": "bun run ./scripts/buildCss.ts",
    "build:icons": "cli icons build --formats woff2,ttf --input ./src/icons/svg --outputFonts ./src/icons/fonts --outputTypes ./src/icons/types.ts",
    "clean": "cli tools clean",
    "dev": "rm -rf ./dist && tsup && concurrently bun:dev:*",
    "dev:icons": "bun run build:icons --watch",
    "dev:src": "tsup --watch",
    "dev:css": "bun run build:css --watch",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "test": "bun test",
    "typecheck": "cli tools typecheck"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-typescript": "^7.23.3",
    "@types/babel__core": "^7.20.5",
    "cli": "workspace:*",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "ts-morph": "^21.0.1",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "webfont": "^11.2.26"
  }
}
