{
  "name": "@katcn/tsmorph",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./tsconfig.json": "./tsconfig.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.cjs",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "cli tools clean",
    "dev": "bun run build --watch",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "test": "bun test",
    "test:update": "bun test --update-snapshots",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "dependencies": {
    "@katcn/types": "workspace:*",
    "ts-morph": "catalog:"
  },
  "devDependencies": {
    "cli": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "catalog:"
  }
}
