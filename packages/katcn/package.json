{
  "name": "katcn",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": [
    "package.json",
    "tsconfig.json",
    "cli",
    "src",
    "dist"
  ],
  "bin": "./cli/bin",
  "exports": {
    ".": {
      "types": "./dist/components.js",
      "default": "./dist/components.js"
    },
    "./cli/*": "./cli/src/*.ts",
    "./node/*": {
      "types": "./dist/node/*.d.ts",
      "default": "./dist/node/*.js"
    },
    "./dtsLibs.json": "./dist/dtsLibs.json",
    "./fixtures/*": {
      "types": "./dist/fixtures/*.d.ts",
      "default": "./dist/fixtures/*.js"
    },
    "./getStyles": {
      "types": "./dist/getStyles.d.ts",
      "default": "./dist/getStyles.js"
    },
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "default": "./dist/helpers/index.js"
    },
    "./getCss": {
      "types": "./dist/getCss.d.ts",
      "default": "./dist/getCss.js"
    },
    "./tokens": {
      "types": "./dist/tokens.d.ts",
      "default": "./dist/tokens.js"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "NODE_ENV=production bun run ./scripts/build.ts",
    "build:icons": "cli icons font --formats woff2,ttf --input ./src/icons/svg --outputFonts ./src/icons/fonts --outputTypes ./src/icons/types.ts --outputData ./src/icons/data.ts",
    "clean": "cli tools clean",
    "dev": "NODE_ENV=development bun run ./scripts/build.ts --watch",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "test": "bun test",
    "test:update": "bun test --update-snapshots",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "prettier": "^3.6.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "cli": "workspace:*",
    "concurrently": "^9.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsconfig": "workspace:*"
  },
  "imports": {
    "#components": {
      "bun": "./src/components.tsx",
      "types": "./src/components.tsx",
      "default": "./dist/components.js"
    },
    "#getCss": {
      "bun": "./src/getCss.ts",
      "types": "./src/getCss.ts",
      "default": "./dist/getCss.js"
    },
    "#getStyles": {
      "bun": "./src/getStyles.ts",
      "types": "./src/getStyles.ts",
      "default": "./dist/getStyles.js"
    },
    "#fixtures/*": {
      "bun": "./src/fixtures/*.ts",
      "types": "./src/fixtures/*.ts",
      "default": "./dist/fixtures/*.js"
    },
    "#tokens": {
      "bun": "./src/tokens.ts",
      "types": "./src/tokens.ts",
      "default": "./dist/tokens.js"
    },
    "#types": {
      "bun": "./src/types.ts",
      "types": "./src/types.ts",
      "default": "./dist/types.d.ts"
    }
  }
}
