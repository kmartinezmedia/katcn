{
  "name": "katcn",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": [
    "package.json",
    "tsconfig.json",
    "cli",
    "src",
    "dist"
  ],
  "bin": "./cli/bin",
  "exports": {
    ".": {
      "types": "./dist/components.js",
      "default": "./dist/components.js"
    },
    "./icons.woff2": "./src/icons/fonts/icons.woff2",
    "./icons.ttf": "./src/icons/fonts/icons.ttf",
    "./macros": {
      "types": "./src/macros/index.ts",
      "default": "./src/macros/index.ts"
    },
    "./macros/*": {
      "types": "./src/macros/*.ts",
      "default": "./src/macros/*.ts"
    },
    "./fixtures": {
      "types": "./dist/fixtures.d.ts",
      "default": "./dist/fixtures.js"
    },
    "./getStyles": {
      "types": "./dist/getStyles.d.ts",
      "default": "./dist/getStyles.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "default": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "default": "./dist/jsx-dev-runtime.js"
    },
    "./tokens": {
      "types": "./dist/tokens.d.ts",
      "default": "./dist/tokens.js"
    },
    "./types": "./dist/types.d.ts",
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "NODE_ENV=production bun run ./scripts/build.ts && bun run build:css && bun run build:icons",
    "build:css": "bun run ./scripts/buildCss.ts",
    "build:icons": "cli icons build --formats woff2,ttf --input ./src/icons/svg --outputFonts ./src/icons/fonts --outputTypes ./src/icons/types.ts",
    "clean": "cli tools clean",
    "dev": "bun run build && concurrently bun:dev:*",
    "dev:css": "bun run build:css --watch",
    "dev:icons": "bun run build:icons --watch",
    "dev:src": "bun run build --watch",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "test": "bun test",
    "typecheck": "cli tools typecheck"
  },
  "dependencies": {
    "lightningcss": "^1.24.0",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "ts-morph": "^22.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.0",
    "cli": "workspace:*",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tsconfig": "workspace:*",
    "webfont": "^11.2.26"
  },
  "imports": {
    "#tokens": {
      "types": "./src/tokens/index.ts",
      "default": "./src/tokens/index.ts"
    }
  }
}
