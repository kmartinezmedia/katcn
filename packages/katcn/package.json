{
  "name": "katcn",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "files": [
    "package.json",
    "tsconfig.json",
    "cli",
    "src",
    "dist"
  ],
  "bin": "./cli/bin",
  "exports": {
    ".": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.js"
    },
    "./cli/*": "./cli/src/*.ts",
    "./dtsLibs.json": "./dist/dtsLibs.json",
    "./tailwindCss.json": "./dist/tailwindCss.json",
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json",
    "./helpers": {
      "types": "./dist/helpers/index.d.ts",
      "import": "./dist/helpers/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "bun run ./scripts/build.ts",
    "clean": "cli tools clean",
    "dev": "bun run build --watch",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "test": "bun test",
    "test:update": "bun test --update-snapshots",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "dependencies": {
    "@katcn/fixtures": "workspace:*",
    "@katcn/types": "workspace:*",
    "clsx": "catalog:",
    "tailwindcss": "catalog:",
    "tailwind-merge": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "bluebun": "catalog:",
    "cli": "workspace:*",
    "concurrently": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "prettier": "catalog:",
    "tsconfig": "workspace:*",
    "ts-morph": "catalog:",
    "tsup": "catalog:"
  },
  "peerDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "imports": {
    "#components": {
      "bun": "./src/components.tsx",
      "types": "./src/components.tsx",
      "default": "./dist/components.js"
    },
    "#getTailwindCss": {
      "bun": "./src/getTailwindCss.ts",
      "types": "./src/getTailwindCss.ts",
      "default": "./dist/getTailwindCss.js"
    },
    "#getStyles": {
      "bun": "./src/getStyles.ts",
      "types": "./src/getStyles.ts",
      "default": "./dist/getStyles.js"
    },
    "#tokens": {
      "bun": "./src/tokens.ts",
      "types": "./src/tokens.ts",
      "default": "./dist/tokens.js"
    }
  }
}
