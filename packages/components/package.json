{
  "name": "katcn",
  "version": "0.0.0",
  "private": true,
  "files": ["tsconfig.json", "src"],
  "type": "module",
  "scripts": {
    "build": "tsup --config ./tsup-runtime.config.ts && tsup",
    "build:icons": "cli icons build --formats woff2,ttf --input ./src/icons/svg --outputFonts ./src/icons/fonts --outputTypes ./src/icons/types.ts"
  },
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./jsx-runtime": "./dist/jsx-runtime.js",
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "cli": "workspace:*",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "webfont": "^11.2.26"
  }
}
