{
  "name": "katcn",
  "version": "0.0.0",
  "private": true,
  "files": ["tsconfig.json", "src", "dist"],
  "type": "module",
  "scripts": {
    "dev": "concurrently bun:dev:*",
    "dev:runtime": "bun run build:runtime --watch",
    "dev:components": "bun run build:components --watch",
    "build": "bun run build:icons && bun run build:runtime && bun run build:components",
    "build:runtime": "tsup --config ./tsup-runtime.config.ts",
    "build:components": "tsup",
    "build:icons": "cli icons build --formats woff2,ttf --input ./src/icons/svg --outputFonts ./src/icons/fonts --outputTypes ./src/icons/types.ts"
  },
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "cli": "workspace:*",
    "concurrently": "^8.2.2",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "webfont": "^11.2.26"
  }
}
