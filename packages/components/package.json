{
  "name": "katcn",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "build": "bun run scripts/build.ts",
    "build:icons": "cli icons build --formats woff2,ttf --input ./src/icons/svg --outputFonts ./src/icons/fonts --outputTypes ./src/icons/types.ts",
    "clean": "cli tools clean"
  },
  "files": ["package.json", "tsconfig.json", "src", "typings", "dist", "dts"],
  "exports": {
    "./tailwind": {
      "types": "./dts/tailwind/index.d.ts",
      "default": "./dist/tailwind/index.js"
    },
    "./*": {
      "types": "./dts/*.d.ts",
      "default": "./dist/*.js"
    },
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-typescript": "^7.23.3",
    "clsx": "^2.1.0",
    "tailwind-merge": "^2.2.1",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "cli": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.2",
    "webfont": "^11.2.26"
  }
}
