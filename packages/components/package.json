{
  "name": "katcn",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "build": "bun run scripts/build.ts",
    "build:icons": "cli icons build --formats woff2,ttf --input ./src/icons/svg --outputFonts ./src/icons/fonts --outputTypes ./src/icons/types.ts",
    "clean": "cli tools clean"
  },
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "tailwindcss": "^3.4.1",
    "tailwind-merge": "^2.2.1",
    "ts-morph": "^21.0.1"
  },
  "devDependencies": {
    "cli": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^8.0.2",
    "webfont": "^11.2.26"
  }
}
