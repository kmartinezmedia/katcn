{
  "include": ["src", "typings"],
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": "src",
    "rootDir": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "katcn",
    "lib": ["ESNext"],
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "emitDeclarationOnly": true,
    "incremental": true,
    "tsBuildInfoFile": "dist/tsconfig.tsbuildinfo"
  }
}
