{
  "extends": "tsconfig/react-library.json",
  "exclude": [
    "shared-config.ts",
    "package-config.ts",
    "runtime-config.ts",
    "cli",
    "configs",
    "dist"
  ],
  "ignore": ["configs"],
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "jsx": "react-jsx",
    "jsxImportSource": "katcn",
    "tsBuildInfoFile": "dist/tsconfig.tsbuildinfo",
    "declarationMap": false
  }
}
