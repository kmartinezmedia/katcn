{
  "name": "docgen",
  "version": "0.0.1",
  "description": "A simple documentation generator",
  "private": true,
  "type": "module",
  "files": ["package.json", "tsconfig.json", "src", "dist", "dts"],
  "exports": {
    ".": {
      "types": "./dts/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dts/*.d.ts",
      "default": "./dist/*.js"
    },
    "./tsconfig.json": "./tsconfig.json",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "clean": "cli tools clean",
    "dev": "tsup --watch",
    "fix": "cli tools fix",
    "lint": "cli tools lint",
    "test": "bun test",
    "typecheck": "cli tools typecheck"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "katcn": "workspace:*",
    "monaco-editor": "^0.46.0",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "cli": "workspace:*",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "next": "14.1.1"
  }
}
